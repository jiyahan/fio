{
    "name": "fio",
    "description": "A minimal D application.",
    "copyright": "Copyright Â© 2015, igor",
    "authors": ["igor"],
    "configurations": [
        {
            "name": "app",
            "targetType": "executable",
            "sourcePaths": ["source", "source/fio"]
        },
        {
            "name": "examples",
            "targetType": "executable",
            "subPackages": [
                {
                    "name": "forked_server",
                    "targetType": "executable",
                    "sourcePaths": ["examples", "source/fio"],
                    "sourceFiles": ["examples/forked_server.d", "source/fio"],
                    "excludedSourceFiles": ["source/app.d"]
                }
            ]
        }
    ],
    "dependencies": {
    },
    "subPackages": [
        {
            "name": "forked_server",
            "targetType": "executable",
            "sourcePaths": ["examples", "source/fio"],
            "sourceFiles": ["examples/forked_server.d", "source/fio/fio.d"],
            "excludedSourceFiles": ["source/app.d"]
        }
    ],
    "buildTypes": {
        "unittests": {
            "buildOptions": ["debugMode", "debugInfo"]
        }
    }
}
